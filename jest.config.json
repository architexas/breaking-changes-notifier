{
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}",
    "!src/interfaces/*",
    "!src/__tests__/**/*",
    "!src/**/__tests__/**/*",
    "!src/images.d.ts",
    "!src/models/*",
    "!src/seeders/*",
    "!src/index.tsx"
  ]
}
